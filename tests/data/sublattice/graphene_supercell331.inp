&GLOBAL
  PRINT_LEVEL Medium
  PROJECT_NAME ortho
  RUN_TYPE ENERGY
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &SUBSYS
    &CELL
      A 12.824260800000001 -0.00000000 0.00000000  
      B 0.00000000 7.4040903 0.00000000 
      C 0.0 0.0 19.998293
      PERIODIC XYZ
    &END CELL
    &TOPOLOGY
      COORD_FILE_FORMAT XYZ
      COORD_FILE_NAME structure_ortho.xyz
    &END TOPOLOGY
    &KIND C
      POTENTIAL GTH-PBE-q4
      BASIS_SET DZVP-MOLOPT-GTH
    &END KIND
    &KIND H
      POTENTIAL GTH-PBE-q1
      BASIS_SET DZVP-MOLOPT-GTH
    &END KIND
  &END SUBSYS
  &DFT
    POTENTIAL_FILE_NAME GTH_POTENTIALS
    BASIS_SET_FILE_NAME BASIS_MOLOPT

    &PRINT
		!&AO_MATRICES
		!	OVERLAP T
		!	KOHN_SHAM_MATRIX T
		!	LOG_PRINT_KEY T
		!	FILENAME KSHamiltonian
		!	&EACH
		!		QS_SCF -1
		!	&END EACH
		!&END AO_MATRICES
		!&MO
        !	COEFFICIENTS T
         !	ENERGIES T
         !	FILENAME MolOrb
         !	MO_INDEX_RANGE 136 145
         !	&EACH
          !   	QS_SCF -1
          !	&END EACH
      	!&END MO

		&KS_CSR_WRITE
			LOG_PRINT_KEY T
			FILENAME KS
			REAL_SPACE F
			&EACH
				QS_SCF -1
			&END EACH
		&END KS_CSR_WRITE

		&S_CSR_WRITE
			LOG_PRINT_KEY T
			FILENAME OVERLAP
			REAL_SPACE F
			&EACH
				QS_SCF -1
			&END EACH
		&END S_CSR_WRITE

      !&BAND_STRUCTURE
      !  ADDED_MOS 35
      !  FILE_NAME ortho.bs
      !  &KPOINT_SET
      !    UNITS B_VECTOR 
      !    SPECIAL_POINT 0.0   0.0   0.0
      !    SPECIAL_POINT 0.5   0.0   0.0
      !    NPOINTS 10
      !  &END
      !&END BAND_STRUCTURE

    &END PRINT
    
    &MGRID
      REL_CUTOFF 60
      NGRIDS 4
      CUTOFF 300
    &END MGRID
    
    &XC
      &XC_FUNCTIONAL PBE
      &END XC_FUNCTIONAL
    &END XC
    
    &KPOINTS
		SCHEME GENERAL
		KPOINT 0. 0. 0. 	0.09
		KPOINT 0.1 0. 0. 	0.18
		KPOINT 0.2 0. 0. 	0.18
		KPOINT 0.3 0. 0. 	0.18
		KPOINT 0.4 0. 0. 	0.18
		KPOINT 0.5 0. 0. 	0.18
      	SYMMETRY OFF 
      	WAVEFUNCTIONS COMPLEX   
    &END KPOINTS
    
    &POISSON
      PERIODIC XYZ
      POISSON_SOLVER PERIODIC
    &END POISSON
    
    &SCF
      EPS_SCF 1e-07
      MAX_SCF 600
      MAX_ITER_LUMO 1000
      SCF_GUESS RESTART
      ADDED_MOS=40
      &DIAGONALIZATION
        EPS_ADAPT 0.01
        ALGORITHM STANDARD
        MAX_ITER 100
      &END DIAGONALIZATION
      &MIXING T
        METHOD BROYDEN_MIXING
        BETA 1.5
        ALPHA 0.3
        NBUFFER 8
      &END MIXING
    &END SCF
  &END DFT
&END FORCE_EVAL
